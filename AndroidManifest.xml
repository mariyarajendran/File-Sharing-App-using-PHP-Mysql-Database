<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sharingfile"
    android:versionCode="1"
    android:versionName="1.0" >

     <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    
    
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sharingfile.LoginArea"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sharingfile.AllViews"
            android:label="@string/title_activity_all_views" >
        </activity>
        <activity
            android:name="com.example.sharingfile.LoadVideoImageAudio"
            android:label="@string/title_activity_load_video_image_audio" >
        </activity>
        <activity
            android:name="com.example.sharingfile.DynamicTables"
            android:label="@string/title_activity_dynamic_tables" >
        </activity>
        <activity
            android:name="com.example.sharingfile.Dynamictables"
            android:label="@string/title_activity_dynamictables" >
        </activity>
        <activity
            android:name="com.example.sharingfile.Loadvidimg"
            android:label="@string/title_activity_loadvidimg" >
        </activity>
        <activity
            android:name="com.example.sharingfile.SharingFile"
            android:label="@string/title_activity_login_area" >
        </activity>
        <activity
            android:name="com.example.sharingfile.SingleList"
            android:label="@string/title_activity_single_list" >
        </activity>
        <activity
            android:name="com.example.sharingfile.Uplaodvideofile"
            android:label="@string/title_activity_uplaodvideofile" >
        </activity>
    </application>

</manifest>
